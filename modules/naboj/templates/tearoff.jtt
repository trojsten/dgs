(@ extends 'base-tearoff.jtt' @)

(@ set target = 'tearoff' @)

(@ block content @)
    \newgeometry{
        paperwidth  = 210mm,
        paperheight = 99mm,
        left        = 12mm,
        right       = 5mm,
        bottom      = 3mm,
        top         = 0mm,
    }
    \pagenumbering{gobble}
    \pagestyle{tearoff}

    (@ for problem in volume.problems @)
        (@ include 'tearoff/problem.jtt' @)
        \newpage
    (@ endfor @)
    (@ include 'tearoff/bottom.jtt' @)
(@ endblock @)
